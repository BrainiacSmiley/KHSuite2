/* doctors badge */
.doctorsbadgesidebar a {
  text-decoration: none;
  color: #000;
}

.doctorsbadgesidebar a:hover {
  text-decoration: none;
  color: #000;
}

.doctorsbadgesidebar a:visited {
  text-decoration: none;
  color: #000;
}

.doctorsbadge {
  background-image:url(<%= asset_path 'doctors/doctors_badge.png' %>);
  background-repeat:no-repeat;
  margin-top: -19px;
  vertical-align: top;
}
.doctorsserver {
  padding-top: 15px;
  margin-left: 153px;
}

.doctorsname {
  margin-top: -15px;
  margin-left: 10px;
}

.doctorsavatar {
  margin-top: -15px;
  margin-left: 174px;
}

.doctorsvalues {
  margin-top: -70px;
  margin-left: 10px;
  line-height: 1;
}

.doctorstimestamp {
  display: block;
  font-size: 85%;
  color: #666;
  margin-top: 2px;
}

.doctorslinks {
  margin-top: -10px;
